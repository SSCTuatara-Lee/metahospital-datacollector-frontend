.camera-panel {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.camera-btn-warpper {
  width: 100%;
  text-align: center;
}
.camera-btn {
  width: 50%;
  height: 8vh;
  line-height: 8vh;
  margin: auto;
  margin-top: 2vh;
  color: white;
  background: #57bead;
  border-radius: 4rpx;
  text-align: center;
}